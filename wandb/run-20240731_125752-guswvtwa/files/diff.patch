+-    def AddToOpenList (self, val):
+-        row,col = val
+-        self.openList.append( (row, col) )
+-        
+-    def RemoveFromOpenList (self, val ):
+-        row,col = val
+-        self.openList.remove( (row, col) )
+-        
+-    def IsInOpenList (self, val ):
